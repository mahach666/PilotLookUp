<UserControl x:Class="PilotLookUp.View.UserControls.CastomObjBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:PilotLookUp.View.UserControls"
             mc:Ignorable="d"
             d:DesignHeight="450"
             d:DesignWidth="800"
             Height="80"
             MinHeight="80"
             MaxHeight="80">
    <Grid>
        <Label Content="{Binding Name}"
               Margin="60,10,100,0"
               VerticalAlignment="Top"
               FontSize="15" />
        <Label Content="{Binding StringId}"
               Margin="60,0,100,10"
               VerticalAlignment="Bottom"
               FontSize="15" />
        <Button Content="Button"
                HorizontalAlignment="Right"
                Margin="0,20,60,0"
                VerticalAlignment="Top"
                Height="40"
                Width="40"
                MinWidth="40"
                MinHeight="40"
                MaxWidth="40"
                MaxHeight="40" />
        <Button Content="Button"
                Margin="0,20,20,0"
                VerticalAlignment="Top"
                Height="40"
                HorizontalAlignment="Right"
                Width="40"
                MinWidth="40"
                MinHeight="40"
                MaxWidth="40"
                MaxHeight="40" />

    </Grid>
</UserControl>

<ResourceDictionary  
                     xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                     xmlns:vm="clr-namespace:PilotLookUp.ViewModel"
                     xmlns:view="clr-namespace:PilotLookUp.View"
                     xmlns:castomview="clr-namespace:PilotLookUp.View.CastomUIElemens"
                    xmlns:page="clr-namespace:PilotLookUp.View.UserControls">

    <DataTemplate DataType="{x:Type vm:ListItemVM}">
        <castomview:ListItemBox />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SearchResVM}">
        <castomview:SearchResBox />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SearchVM}">
        <page:SearchPage />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:LookUpVM}">
        <page:LookUpPage />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:TaskTreeVM}">
        <page:TaskTreeViewPage />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AttrVM}">
        <page:AttrPage />
    </DataTemplate>

</ResourceDictionary>

